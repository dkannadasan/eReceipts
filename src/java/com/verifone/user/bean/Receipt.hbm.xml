<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.verifone.model.Receipt" table="receipt" lazy="false" >
	 <id name="gkey" column="id" type="java.lang.Integer">
		 <generator class="native"/>
	 </id>
	 <property name="receiptId" column="receiptId" type="java.lang.String" />
	 <property name="receiptDate" column="receiptDate" type="java.util.Date" />
	 <property name="userId" column="userId" type="java.math.BigInteger" />
</class>
  

</hibernate-mapping>
